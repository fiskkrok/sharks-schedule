{
    "routes": [
        {
            "route": "/api/v1/club-schedule-season/*",
            "rewrite": "https://api-web.nhle.com/v1/club-schedule-season/{*}",
            "methods": [
                "GET",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, OPTIONS",
                "Access-Control-Allow-Headers": "*",
                "Cache-Control": "no-cache"
            }
        },
        {
            "route": "/api/v1/player/*",
            "rewrite": "https://api-web.nhle.com/v1/player/{*}",
            "methods": [
                "GET",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, OPTIONS",
                "Access-Control-Allow-Headers": "*",
                "Cache-Control": "no-cache"
            }
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*",
            "/api/*"
        ]
    },
    "globalHeaders": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://api-web.nhle.com https://cdn.nhle.com; img-src 'self' https://cdn.nhle.com data:; connect-src 'self' https://api-web.nhle.com;"
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}