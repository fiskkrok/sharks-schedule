{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/assets/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "rewrite": "https://api-web.nhle.com/{rest}"
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "cache-control": "must-revalidate, max-age=0"
    }
}