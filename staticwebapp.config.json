{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*"
        ]
    },
    "routes": [
        {
            "route": "/api/v1/*",
            "rewrite": "https://api-web.nhle.com/v1/{rest}",
            "methods": [
                "GET"
            ],
            "headers": {
                "Cache-Control": "no-cache",
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "*"
            }
        },
        {
            "route": "/api/logos/*",
            "rewrite": "https://www-league.nhlstatic.com/{rest}",
            "methods": [
                "GET"
            ],
            "headers": {
                "Cache-Control": "no-cache",
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "*"
            }
        }
    ],
    "platformErrorOverrides": [
        {
            "errorType": "NotFound",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}