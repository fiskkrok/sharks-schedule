{
    "routes": [
        {
            "route": "/api/v1/club-schedule-season/*",
            "rewrite": "https://api-web.nhle.com/v1/club-schedule-season/{0}/{1}",
            "methods": [
                "GET"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "*",
                "Cache-Control": "no-cache"
            }
        },
        {
            "route": "/api/v1/player/*/landing",
            "rewrite": "https://api-web.nhle.com/v1/player/{0}/landing",
            "methods": [
                "GET"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET",
                "Access-Control-Allow-Headers": "*",
                "Cache-Control": "no-cache"
            }
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*"
        ]
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://api-web.nhle.com https://cdn.nhle.com; img-src 'self' https://cdn.nhle.com data:; connect-src 'self' https://api-web.nhle.com;",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "*"
    }
}