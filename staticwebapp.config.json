{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/assets/*.{png,jpg,gif,svg,css,js}",
            "/*.{ico,json}"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "rewrite": "https://api-web.nhle.com/{rest}"
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "cache-control": "no-cache",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}